	$(HTML2TEXT) $(srcdir)/doc/www.lesstif.org/INSTALL.html >$@

#
# Daily is a target to create a normal distribution but then rename it.
#
daily:	dist
#	mv lesstif-0.95.0.tar.gz lesstif-daily.tar.gz
	tar xfz lesstif-0.95.0.tar.gz
	mv lesstif-0.95.0 lesstif-daily
	tar cfz lesstif-daily.tar.gz lesstif-daily
	-rm -rf lesstif-daily

# One target for each version and an unified one
ctags-1.2:
	$(CTAGS_CMD) \
        include/*.h \
        include/Motif-1.2 \
        lib/Xm \
        lib/Mrm \
        lib/Uil \
        clients/Motif-1.2

ctags-2.0:
	$(CTAGS_CMD) \
        include/*.h \
        include/Motif-2.0 \
        lib/Xm-2.0 \
        lib/Mrm-2.0 \
        lib/Uil-2.0 \
        clients/Motif-2.0

ctags-2.1:
	$(CTAGS_CMD) \
        include/*.h \
        include/Motif-2.1 \
        lib/Xm-2.1 \
        lib/Mrm-2.1 \
        lib/Uil-2.1 \
        lib/Dt \
        clients/Motif-2.1

ctags-all:
	$(CTAGS_CMD) \
        include/*.h \
        include/Motif-2.1  include/Motif-2.0  include/Motif-1.2 \
        lib/Xm-2.1         lib/Xm-2.0         lib/Xm \
        lib/Mrm-2.1        lib/Mrm-2.0        lib/Mrm \
        lib/Uil-2.1        lib/Uil-2.0        lib/Uil \
        lib/Dt \
        clients/Motif-2.1  clients/Motif-2.0  clients/Motif-1.2 \
        test/common
